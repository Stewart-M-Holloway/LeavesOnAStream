{
  "name": "mantine-vite-template",
  "private": true,
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "npm run lint:eslint && npm run lint:stylelint",
    "lint-staged": "eslint --cache --fix . && prettier --write .",
    "lint:eslint": "eslint . --ext .ts,.tsx --cache",
    "lint:stylelint": "stylelint '**/*.css' --cache",
    "prettier": "prettier --check \"**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx}\"",
    "vitest": "vitest run",
    "vitest:watch": "vitest",
    "test": "npm run typecheck && npm run prettier && npm run lint && npm run vitest && npm run build",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@mantine/core": "7.13.2",
    "@mantine/hooks": "7.11.2",
    "@tabler/icons-react": "^3.2.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.1",
    "lint-staged": "^15.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-localization": "^1.0.19",
    "react-router-dom": "^6.23.0",
    "react-scroll-parallax": "^3.4.5"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.2.9",
    "@storybook/addon-interactions": "^8.0.6",
    "@storybook/addon-links": "^8.1.3",
    "@storybook/blocks": "^8.0.4",
    "@storybook/react": "^8.1.4",
    "@storybook/react-vite": "^8.2.9",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.36",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.53.0",
    "eslint-config-mantine": "3.2.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.7.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "husky": "^9.1.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.38",
    "postcss-preset-mantine": "^1.13.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.1.1",
    "prop-types": "^15.8.1",
    "storybook": "^8.2.8",
    "storybook-dark-mode": "^4.0.1",
    "stylelint": "^16.3.1",
    "stylelint-config-standard-scss": "^13.0.0",
    "ts-jest": "^29.1.4",
    "typescript": "^5.4.5",
    "vite": "^5.4.8",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.0.5"
  },
  "packageManager": "npm@10.8.2"
}
